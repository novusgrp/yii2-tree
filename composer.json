{
    "name": "novusgrp/yii2-tree",
    "description": "Yii2 Tree Extension",
    "type": "yii2-extension",
    "keywords": ["yii2", "extension"],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "armand@novusgroup.co.za",
            "email": "armand@novusgroup.co.za"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.0",
        "yiisoft/yii2-jui": "*",
        "bower-asset/fancytree": ">=2.12",
        "npm-asset/jquery-contextmenu": "^2.9"
    },
    "autoload": {
        "psr-4": {
            "novusgrp\\": ""
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
